pipeline {
    agent any
    stages {
        stage('Version') {
            steps {
                bat 'py --version'
            }
        }
        stage('Execute stage 1') {
            steps {
                bat 'py ip_print.py input1.json'
            }
        }
        stage('Execute stage 2') {
            steps {
                bat 'py ip_print.py input2.json'
            }
        }
    }
}